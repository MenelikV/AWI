<div class="jumbotron jumbostyle">
    <div class="container-fluid">
        <!-- Title and Activity container-->
        <div class="row">
            <div class="col md-4"></div>
            <div class="col md-4">
                <h1 class="display 4 text-center" style="padding-top:2rem;"> <%=activity%> Flights </h1>
            </div>
            <div class="col md-4">
            </div>
        </div> 
        <hr style="padding-bottom:2rem;">

        <div class="row">
            <div class="col md-6">
                <!-- Flight Overview container -->
                <div class="container-fluid" style="background-color:rgb(255, 255, 255); overflow:auto; padding:10px;">
                    <table id="aircrafts" class="table table-striped text-left table-hover" >
                        <thead>
                            <% for(var i=0; i < headers.length; i++) {%>
                            <th>
                                <%= headers[i] %>
                            </th>
                            <% } %>
                        </thead>

                        <tbody >
                            <% for(var i=0; i < info.length; i++) {%>
                            <tr style="cursor : pointer;">
                                <% for(var j=0; j < headers.length; j++) {%>
                                <td >
                                    <%= info[i][headers[j]] %>
                                </td>
                                <% } %>
                            </tr>
                            <% } %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>